{
  "name": "datadog-export-checkly-migration",
  "version": "1.0.0",
  "description": "Export Datadog synthetics and global variables for migration to Checkly",
  "type": "module",
  "main": "src/01-initial-datadog-export.ts",
  "scripts": {
    "export": "ts-node src/01-initial-datadog-export.ts",
    "convert:api": "ts-node src/02-convert-datadog-api-to-json.ts",
    "filter-multi": "ts-node src/03-filter-multi-step-from-api-json.ts",
    "generate:checkly": "ts-node src/04-generate-api-check-constructs-from-json.ts",
    "migrate:api": "npm run convert:api && npm run generate:checkly",
    "generate:multi-specs": "ts-node src/05-generate-multi-step-specs.ts",
    "generate:multi-checks": "ts-node src/06-generate-multi-step-constructs.ts",
    "migrate:multi": "npm run generate:multi-specs && npm run generate:multi-checks",
    "generate:browser-specs": "ts-node src/07-generate-browser-specs.ts",
    "generate:browser-checks": "ts-node src/08-generate-browser-constructs.ts",
    "migrate:browser": "npm run generate:browser-specs && npm run generate:browser-checks",
    "convert:variables": "ts-node src/09-convert-global-variables.ts",
    "create:variables": "ts-node checkly-migrated/variables/create-variables.ts",
    "delete:variables": "ts-node checkly-migrated/variables/delete-variables.ts",
    "add:defaults": "ts-node src/10-add-default-resources.ts",
    "generate:groups": "ts-node src/11-generate-groups.ts",
    "migrate:all": "npm run export && npm run filter-multi && npm run migrate:api && npm run migrate:multi && npm run migrate:browser && npm run convert:variables && npm run generate:groups && npm run add:defaults"
  },
  "keywords": [
    "datadog",
    "checkly",
    "synthetics",
    "migration"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "checkly": "^6.9.10",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "jiti": "^2.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
